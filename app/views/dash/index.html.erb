  <div class="container-fluid" ng-controller="MainSailingController">
    <div class="row" sailing-data>
      <div class="col-md-9 map_container" style="height:600px;" sailing-map>
        <div id="map_canvas" class="maxHeight">

        </div>
      </div>

      <div class="col-md-3 sidebar maxHeight">
        <ul>
          <li><div class = "start_buttons">
            <button ng-click ="$parent.startSelect=hut;">Hut Start</button> 
            <button ng-click ="$parent.startSelect=communal;"> Communal Start</button>
          <li ng-repeat="course in courses"> <button ng-click="$parent.selectedCourse=$index;addMarksAndLine();"> Course {{ course.symbol }} </button> </li>
        </ul>
      </div>
      

    </div>
  </div>
  <div class = "col-md-12 weather">
        <p id="weatherStuff"></p>
  </div>
